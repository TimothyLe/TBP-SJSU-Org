<div class="modal fade bs-modal-sm" id="myModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-md">
    <div class="modal-content">
      <br>
      <div class="bs-example bs-example-tabs">
        <ul id="myTab" class="nav nav-tabs">
          <li class="active"><a href="#signin" data-toggle="tab">Student</a></li>
          <li class=""><a href="#why" data-toggle="tab">Help/FAQ?</a></li>
        </ul>
      </div>
      <div class="modal-body">
        <div id="myTabContent" class="tab-content">
          <div class="tab-pane fade active in" id="signin">
            <%
            def resource_name
              :user
            end

            def resource
              @resource ||= User.new
            end

            def devise_mapping
              @devise_mapping ||= Devise.mappings[:user]
            end
            %>
            <%= render 'devise/sessions/login_session' %>
          </div>
          <div class="tab-pane fade" id="signup">
            <%
            def resource_name
              :teacher
            end

            def resource
              @resource ||= Teacher.new
            end

            def devise_mapping
              @devise_mapping ||= Devise.mappings[:teacher]
            end
            %>
            <%= render 'devise/sessions/login_session' %>  
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <center>
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </center>
      </div>
    </div>
  </div>
</div>
